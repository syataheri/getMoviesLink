<!DOCTYPE html>
<html lang="en">

<head>
    <meta name="viewport" content="width=device-width" />
    <title>Get Movies Link</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css"
        integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
</head>

<body>
    <div class="container body-content">
        <div class="container">
            <form action="/search" method="post">
                <div class="row justify-content-start" style="padding-top:10px;">
                    <h1>Get Movies Links</h1>
                </div>
                <div class="row justify-content-center" style="padding-top:10px;">
                    <input class="form-control" name="movieName" id="movieName"
                        placeholder="Enter movie's name"></input>
                </div>
                <div class="row justify-content-start" style="padding-top:10px;">
                    <button class="btn btn-primary" type="submit">Search</button>
                </div>
            </form>
            <% if(Object.keys(links).length !==0){ %>
                <div class="row justify-content-start" style="padding-top: 3.5rem;;">
                    <% if(links.moviecotteg !==undefined && links.moviecotteg.length> 0) { %>
                        <div class="row justify-content-start" style="padding-top:10px;">
                            <h3>moviecotteg Links</h3>
                            <ul style="padding-top: 2rem;">
                                <% for(link of links.moviecotteg){ %>
                                    <li><a href="<%= link %>">
                                            <%= link %>
                                        </a></li>
                                    <% } %>
                            </ul>
                        </div>
                        <% } %>

                            <% if(links.ifilmz !==undefined && links.ifilmz.length> 0) { %>
                                <div class="row justify-content-start" style="padding-top:10px;">
                                    <h3>ifilmz Links</h3>
                                    <ul style="padding-top: 2rem;">
                                        <% for(link of links.ifilmz){ %>
                                            <li><a href="<%= link %>">
                                                    <%= link %>
                                                </a></li>
                                            <% } %>
                                    </ul>
                                </div>
                                <% } %>

                                    <% if(links.myfilm !==undefined && links.myfilm.length> 0) { %>
                                        <div class="row justify-content-start" style="padding-top:10px;">
                                            <h3>myfilm Links</h3>
                                            <ul style="padding-top: 2rem;">
                                                <% for(link of links.myfilm){ %>
                                                    <li><a href="<%= link %>">
                                                            <%= link %>
                                                        </a></li>
                                                    <% } %>
                                            </ul>
                                        </div>
                                        <% } %>

                                            <% if(links.vipofilm !==undefined && links.vipofilm.length> 0) { %>
                                                <div class="row justify-content-start" style="padding-top:10px;">
                                                    <h3>vipofilm Links</h3>
                                                    <ul style="padding-top: 2rem;">
                                                        <% for(link of links.vipofilm){ %>
                                                            <li><a href="<%= link %>">
                                                                    <%= link %>
                                                                </a></li>
                                                            <% } %>
                                                    </ul>
                                                </div>
                                                <% } %>
                </div>
                <% } %>
        </div>
    </div>

    </div>
    </div>
    </div>
    </div>
</body>

</html>